    "lint": "run-p lint:eslint lint:markdown",
    "lint:eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:markdown": "markdownlint --ignore dist --ignore examples --ignore node_modules '**/*.md' '.**/**/*.md'"
